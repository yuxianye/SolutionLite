<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="Level">日志等级</sys:String>
    <sys:String x:Key="ThreadId">线程号</sys:String>
    <sys:String x:Key="Message">信息</sys:String>
    <sys:String x:Key="CallSite">来源</sys:String>
    <sys:String x:Key="Exception">异常</sys:String>
    <sys:String x:Key="StackTrace">堆栈</sys:String>
    <sys:String x:Key="AppLogSearchTextBoxWatermark">输入信息内容</sys:String>
    <sys:String x:Key="IsAutoRefresh">自动刷新</sys:String>
    <sys:String x:Key="IsAutoRefreshToolTip">选中后自动刷新半小时内的日志，刷新周期参见配置文件中的AppLogAutoRefreshInterval项</sys:String>

</ResourceDictionary>